
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Cliente {
  id              String      @id @default(auto()) @map("_id") @db.ObjectId
  nome            String
  cpf             String      @unique
  data_nascimento DateTime?
  logradouro      String
  num_casa        String
  bairro          String
  complemento     String?
  municipio       String
  uf              String
  telefone        String
  email           String
}